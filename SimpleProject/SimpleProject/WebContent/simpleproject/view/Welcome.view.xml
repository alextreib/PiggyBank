<mvc:View
  controllerName="simpleproject.controller.Welcome"
   xmlns:mvc="sap.ui.core.mvc"
  xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core"
	xmlns:f="sap.ui.layout.form"
	 xmlns:html="http://www.w3.org/1999/xhtml"
	 xmlns:t ="sap.ui.table"
  xmlns="sap.m"
  xmlns:smartTable="sap.ui.comp.smarttable">
	<l:VerticalLayout class="sapUiSmallMargin">
		<l:HorizontalLayout>
			<l:VerticalLayout>
			<Text text="Welcome User" />
			<TextArea text="Hier steht unser Log-Element irgendwann"></TextArea>
			</l:VerticalLayout>
		</l:HorizontalLayout>
			<l:Grid defaultSpan="L4 M6 S6" class="sapUiSmallMarginTop">
  				<ObjectListItem title="My Accounts" type="Active" icon="sap-icon://activity-items" press="onNav">
  					<layoutData>
  						<l:GridData span="L6 M12 S12" />
  					</layoutData>
  				</ObjectListItem>
  				<ObjectListItem title="My Rules" type="Navigation" icon="sap-icon://activity-individual" press="onMyRules" >
  				<layoutData>
  						<l:GridData span="L6 M12 S12" />
  					</layoutData>
  				</ObjectListItem>
  				<ObjectListItem title="Wishlist" type="Navigation" icon="sap-icon://settings" press="onWishlist">
  				<layoutData>
  						<l:GridData span="L6 M12 S12" />
  					</layoutData>
  				</ObjectListItem>
  				<ObjectListItem title="Consultant" type="Active" icon="sap-icon://log" press="onConsultant">
  				<layoutData>
  						<l:GridData span="L6 M12 S12" />
  					</layoutData>
  				</ObjectListItem>
  				
  			</l:Grid>
		
	</l:VerticalLayout>
	
	


</mvc:View>